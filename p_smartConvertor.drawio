        <mxCell id="9X4YrfVrGWDHbrTyHEu1-31" value="#ifndef MQTTMESSAGE_H&#xa;#define MQTTMESSAGE_H&#xa;&#xa;&#xa;#include &lt;string&gt;&#xa;#include &lt;unordered_map&gt;&#xa;&#xa;&#xa;class MQTTMessage {&#xa;private:&#xa;    std::string topic_;&#xa;    std::unordered_map&lt;std::string, std::string&gt; payload_;&#xa;&#xa;&#xa;public:&#xa;    MQTTMessage(const std::string&amp; topic, const std::unordered_map&lt;std::string, std::string&gt;&amp; payload)&#xa;        : topic_(topic), payload_(payload) {}&#xa;&#xa;&#xa;    std::string getTopic() const {&#xa;        return topic_;&#xa;    }&#xa;&#xa;&#xa;    std::unordered_map&lt;std::string, std::string&gt; getPayload() const {&#xa;        return payload_;&#xa;    }&#xa;};&#xa;&#xa;&#xa;#endif  // MQTTMESSAGE_H" style="text;whiteSpace=wrap;html=1;fillColor=#f5f5f5;fontColor=#333333;strokeColor=#666666;" parent="9X4YrfVrGWDHbrTyHEu1-30" vertex="1">
