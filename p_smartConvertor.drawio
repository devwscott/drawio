        <mxCell id="oeiSj27BiU-lJ-WvPtet-120" value="#include &lt;br&gt;#include &lt;br&gt;#include &lt;br&gt;#include &quot;MQTTMessageParserFactoryImpl.h&quot;&lt;br&gt;#include &quot;MQTTMessageParser.h&quot;&lt;br&gt;#include &quot;MQTTMessage.h&quot;&lt;br&gt;&lt;br&gt;&lt;br&gt;class Application {&lt;br&gt;public:&lt;br&gt;&amp;nbsp; &amp;nbsp; void processMessage(const std::string&amp;amp; topic, const std::string&amp;amp; message) {&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; // MQTT 메시지 파서 팩토리를 생성&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; std::shared_ptr&amp;lt;MQTTMessageParserFactory&amp;gt;&amp;nbsp;factory = std::make_shared();&lt;br&gt;&lt;br&gt;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; // MQTT 메시지 파서를 생성&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; std::shared_ptr&amp;lt;MQTTMessageParser&amp;gt;&amp;nbsp;parser = factory-&amp;gt;createParser();&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; if (parser == nullptr) {&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; std::cout &amp;lt;&amp;lt; &quot;Unsupported topic: &quot; &amp;lt;&amp;lt; topic &amp;lt;&amp;lt; std::endl;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; return;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;br&gt;&lt;br&gt;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; // 메시지 파싱&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; std::shared_ptr&amp;lt;MQTTMessage&amp;gt;&amp;nbsp;mqttMessage = parser-&amp;gt;parse(message);&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; if (mqttMessage == nullptr) {&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; std::cout &amp;lt;&amp;lt; &quot;Failed to parse message for topic: &quot; &amp;lt;&amp;lt; topic &amp;lt;&amp;lt; std::endl;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; return;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;br&gt;&lt;br&gt;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; // 파싱 결과 출력&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; std::cout &amp;lt;&amp;lt; &quot;Topic: &quot; &amp;lt;&amp;lt; mqttMessage-&amp;gt;getTopic() &amp;lt;&amp;lt; std::endl;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; std::unordered_map payload = mqttMessage-&amp;gt;getPayload();&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; for (const auto&amp;amp; pair : payload) {&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; std::cout &amp;lt;&amp;lt; pair.first &amp;lt;&amp;lt; &quot;: &quot; &amp;lt;&amp;lt; pair.second &amp;lt;&amp;lt; std::endl;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;br&gt;&amp;nbsp; &amp;nbsp; }&lt;br&gt;};&lt;br&gt;&lt;br&gt;&lt;br&gt;int main() {&lt;br&gt;&amp;nbsp; &amp;nbsp; Application app;&lt;br&gt;&lt;br&gt;&lt;br&gt;&amp;nbsp; &amp;nbsp; // 테스트용 메시지&lt;br&gt;&amp;nbsp; &amp;nbsp; std::string topic = &quot;light_control&quot;;&lt;br&gt;&amp;nbsp; &amp;nbsp; std::string message = R&quot;({&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &quot;device_id&quot;: &quot;12345&quot;,&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &quot;power&quot;: &quot;on&quot;&lt;br&gt;&amp;nbsp; &amp;nbsp; })&quot;;&lt;br&gt;&lt;br&gt;&lt;br&gt;&amp;nbsp; &amp;nbsp; // 메시지 처리&lt;br&gt;&amp;nbsp; &amp;nbsp; app.processMessage(topic, message);&lt;br&gt;&lt;br&gt;&lt;br&gt;&amp;nbsp; &amp;nbsp; return 0;&lt;br&gt;}" style="text;whiteSpace=wrap;html=1;fillColor=#f5f5f5;fontColor=#333333;strokeColor=#666666;" vertex="1" parent="oeiSj27BiU-lJ-WvPtet-115">
