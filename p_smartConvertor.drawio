        <mxCell id="9X4YrfVrGWDHbrTyHEu1-32" value="#ifndef MQTTMESSAGEPARSER_H&#xa;#define MQTTMESSAGEPARSER_H&#xa;&#xa;&#xa;#include &lt;string&gt;&#xa;#include &lt;memory&gt;&#xa;#include &quot;MQTTMessage.h&quot;&#xa;&#xa;&#xa;class MQTTMessageParser {&#xa;public:&#xa;    virtual std::shared_ptr&lt;MQTTMessage&gt; parse(const std::string&amp; message) = 0;&#xa;};&#xa;&#xa;&#xa;#endif  // MQTTMESSAGEPARSER_H" style="text;whiteSpace=wrap;html=1;fillColor=#f5f5f5;fontColor=#333333;strokeColor=#666666;" parent="9X4YrfVrGWDHbrTyHEu1-30" vertex="1">
