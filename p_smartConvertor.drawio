        <mxCell id="9X4YrfVrGWDHbrTyHEu1-33" value="#ifndef LIGHTCONTROLPARSER_H&lt;br&gt;#define LIGHTCONTROLPARSER_H&lt;br&gt;&lt;br&gt;&lt;br&gt;#include &lt;br&gt;#include &lt;br&gt;#include &quot;MQTTMessageParser.h&quot;&lt;br&gt;&lt;br&gt;&lt;br&gt;using json = nlohmann::json;&lt;br&gt;&lt;br&gt;&lt;br&gt;class LightControlParser : public MQTTMessageParser {&lt;br&gt;public:&lt;br&gt;&amp;nbsp; &amp;nbsp; std::shared_ptr parse(const std::string&amp;amp; message) override {&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; std::unordered_map payload;&lt;br&gt;&lt;br&gt;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; try {&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; json jsonObject = json::parse(message);&lt;br&gt;&lt;br&gt;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; for (auto it = jsonObject.begin(); it != jsonObject.end(); ++it) {&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; if (it-&amp;gt;is_string()) {&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; payload[it.key()] = it-&amp;gt;get();&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; } catch (const std::exception&amp;amp; e) {&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; std::cout &amp;lt;&amp;lt; &quot;Failed to parse JSON: &quot; &amp;lt;&amp;lt; e.what() &amp;lt;&amp;lt; std::endl;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; return nullptr;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; }&lt;br&gt;&lt;br&gt;&lt;br&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; return std::make_shared&amp;lt;MQTTMessage&amp;gt;(&quot;light_control&quot;, payload);&lt;br&gt;&amp;nbsp; &amp;nbsp; }&lt;br&gt;};&lt;br&gt;&lt;br&gt;&lt;br&gt;#endif&amp;nbsp; // LIGHTCONTROLPARSER_H" style="text;whiteSpace=wrap;html=1;fillColor=#f5f5f5;strokeColor=#666666;fontColor=#333333;" parent="9X4YrfVrGWDHbrTyHEu1-30" vertex="1">
