        <mxCell id="lNwOq3O8PZaDgsoWruV2-18" value="#include &quot;mqtt.h&quot;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;//mqtt-library header&lt;br&gt;&lt;br&gt;&lt;font color=&quot;#ff0000&quot;&gt;// class에서 사용하는 struct를 아래와 같이 class위에 정의하고 사용?&lt;/font&gt;&lt;br&gt;&lt;br&gt;struct mbedtls_context {&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;mbedtls_net_context net_ctx;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;mbedtld_ssl_context ssl_ctx;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;mbedtls_ssl_config ssl_conf;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;mbedtls_x509_crt ca_crt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;mbedtls_entropy_context entropy;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;mbedtls_ctr_drbg_context ctr_drbg;&lt;br&gt;};&lt;br&gt;&lt;br&gt;&lt;font color=&quot;#ff0000&quot;&gt;// class에서 enum정의 아래와 같이 사용?&lt;/font&gt;&lt;br&gt;typedef enum{&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;QOS_Level_0,&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;QOS_Level_1,&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;QOS_Level_2&lt;br&gt;}QOS_Level;&lt;br&gt;&lt;br&gt;&lt;br&gt;class IMqttClientListener{&lt;br&gt;public:&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;virtual void onReceiveMqtt(string &amp;amp;topic, string &amp;amp;mqtt) = 0;&lt;br&gt;};&lt;br&gt;&lt;br&gt;&lt;br&gt;class MqttClient {&lt;br&gt;private:&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;IMqttClientListener* listener;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;int m_qosLevel;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;int m_keepAliveTime;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;pthread_t m_thread;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;bool m_connected;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;font color=&quot;#ff0000&quot;&gt;//MQTT-C lib관련 아래 mutex변수 정의/사용가능?&lt;/font&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;pthread_mutex_t m_lock;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;mqtt_client m_mqtt_client;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;// struct mqtt_client in mqtt.h&lt;br&gt;&lt;font color=&quot;#ff0000&quot;&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;// MQTT-C library에서 사용하는 아래 버퍼를 아래와 같이 기존 프토토타입으로&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;//정의하고 class의 private 변수로 할당해서 사용?&lt;/font&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;unsigned char m_recvbuf[512];&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;unsigned char m_sendbuf[512];&lt;br&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;mbedtls_context m_mbedtls_context;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;// defined abve.&lt;br&gt;&lt;br&gt;public:&lt;br&gt;MqttClient(IMqttClientListener *callback){&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;m_connected = false;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;listener = callback;&lt;br&gt;}&lt;br&gt;&lt;br&gt;~MqttClient(){&lt;br&gt;}&lt;br&gt;&lt;br&gt;bool init(int qoslevel, int keepAliveTime){&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;m_qoslevel = qoslevel;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;m_keepAliveTime = keepAliveTime;&lt;br&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;pthread_create(&amp;amp;m_thread, NULL, &amp;amp;MqttClient::syncThread, this);&lt;br&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;pthread_mutex_init(&amp;amp;m_lock, NULL);&lt;br&gt;}&lt;br&gt;&lt;br&gt;bool connect(string broker, string port, string cert){&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;int socketFd;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;mbedtls_open_socket(&amp;amp;m_mbedtls_context);&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;socketFd = &amp;amp;m_mbedtls_context.ssl_ctx;&lt;br&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;mqtt_init(&amp;amp;m_mqtt_client, socketFd, m_sendbuf, m_recvbuf, _mqtt_subCallback);&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;mqtt_connect(&amp;amp;m_mqtt_client, &quot;SC&quot;, will_topic?, xxxx);&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;m_connected = true;&lt;br&gt;}&lt;br&gt;&lt;br&gt;&lt;br&gt;void* syncThread(void *MqttClient){&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;while(1){&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;if(m_connected){&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;pthread_mutex_lock(&amp;amp;m_lock);&lt;br&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;mqtt_sync();&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;pthread_mutex_unlock(&amp;amp;m_lock);&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;}&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;usleep(100*1000);&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;//100ms sleep()&amp;nbsp;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;}&lt;br&gt;}&lt;br&gt;&lt;br&gt;&lt;br&gt;bool publish(string topic, string msg){&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;mqtt_publish(&amp;amp;m_mqtt_client, topic.c_str(), msg.c_str(), m_qosLevel);&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;return true;&lt;br&gt;}&lt;br&gt;&lt;br&gt;&lt;br&gt;&lt;br&gt;bool subscribe(string topic){&lt;br&gt;&lt;font color=&quot;#ff0000&quot;&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;//내부에서 subscribe() topic을 관리해야 하나?&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;//_mqtt_subCallback()으로 상위 layer에 전달할때 sub한 topic인지 확인하는 절차 필요한가?&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;//필요하다면 내부 c++ STL list이용??&lt;/font&gt;&lt;br&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;if(topic != null)&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;mqtt_subscribe(&amp;amp;m_mqtt_client, topic.c_str(), m_qosLevel);&lt;br&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;return true;&lt;br&gt;}&lt;br&gt;&lt;br&gt;&lt;br&gt;bool unSubscribe(string topic){&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;mqtt_unsubscribe(topic.c_str());&lt;br&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;return true;&lt;br&gt;};&lt;br&gt;&lt;font color=&quot;#ff0000&quot;&gt;&lt;br&gt;// _mqtt_subCallback()함수를 class 외부에 정의하고 해당 함수에서 맴버변수 listener접근 가능?&lt;br&gt;// 아래 함수이외에 mbedtls open socket()관련 함수도 class외부에 정의하고 사용가능? 이때 class meber 변수 접근가능?&lt;br&gt;&lt;/font&gt;static void _mqtt_subCallback(void** unused, struct mqtt_response_publish * published){&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;char* topic = malloc();&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;memcpy(topic, pubished-&amp;gt;topic, xxx);&lt;br&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;char* msg = malloc();&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;memcpy(msg, published-&amp;gt;msg, xxx);&lt;br&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;string *topic_str = new string(topic);&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;string *msg_str = new string(msg);&lt;br&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;free(topic);&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;free(msg);&lt;br&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;font color=&quot;#ff0000&quot;&gt;// listener로 topic, msg 전달시 아래와 같이 string new()로 전달가능?&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;//onReceiveMqtt(string &amp;amp;topic, string &amp;amp;msg) --&amp;gt; 프로토타입?맞나?&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;//아니면 char*로 data전달하는게 맞나?&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;//onReceiveMqtt(char* topic, char* msg);&lt;/font&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;listener-&amp;gt;onReceiveMqtt(topic_str, msg_str);&lt;br&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;delete topic_str;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;delete msg_str;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;br&gt;}" style="text;html=1;strokeColor=default;fillColor=default;align=left;verticalAlign=top;whiteSpace=wrap;rounded=0;fontSize=11;fontFamily=Helvetica;fontColor=default;" parent="lNwOq3O8PZaDgsoWruV2-15" vertex="1">
