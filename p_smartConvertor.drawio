        <mxCell id="qgbftZ3iaYtYM-1pswJt-37" value="&lt;br&gt;class IInterCoreCommtListener{&lt;br&gt;public:&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;virtual void onReceiveInterCore(char* data, int size) = 0;&lt;br&gt;};&lt;br&gt;&lt;br&gt;&lt;br&gt;class InterCoreComm {&lt;br&gt;private:&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;IInterCoreCommListener* listener;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;pthread_t m_thread;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;bool m_running;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;br&gt;&lt;br&gt;public:&lt;br&gt;InterCoreComm(IInterCoreCommListener *callback){&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;m_running = false;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;listener = callback;&lt;br&gt;}&lt;br&gt;&lt;br&gt;~InterCoreComm(){&lt;br&gt;}&lt;br&gt;&lt;br&gt;bool init(char* coreID, int recvTime){&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;m_sockFd = Application_Connect(coreID);&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;setsockopt(m_sockFd, SOL_SOCKET, SO_RCVTIMEO, recvTimeout,xx);&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;// EventLoop or epoll() 사용???&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;m_eventloop = EventLoop_Create();&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;EventLoop_RegisterIo(m_eventloop, m_sockFd, EventLoop_Input, _SocketEventHandler);&lt;br&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;pthread_create(&amp;amp;m_thread, NULL, &amp;amp;InterCoreComm::loopThread, this);&lt;br&gt;}&lt;br&gt;&lt;br&gt;void run(){&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;m_running = true;&lt;br&gt;}&lt;br&gt;&lt;br&gt;void* loopThread(void *InterCoreComm){&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;while(1){&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;if(m_running){&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; EventLoop_Run(m_eventloop,xxx);&lt;/span&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;}&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;}&lt;br&gt;}&lt;br&gt;&lt;br&gt;&lt;br&gt;bool send(char* data, int size){&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;send(m_sockFd, data, size);&lt;br&gt;}&lt;br&gt;&lt;br&gt;};&lt;br&gt;&lt;br&gt;&lt;br&gt;&lt;br&gt;&lt;br&gt;&lt;font color=&quot;#ff0000&quot;&gt;// _SocketEventHandler()함수를 class 외부에 정의하고 해당 함수에서 맴버변수 listener접근 가능?&lt;/font&gt;&lt;br&gt;static _SocketEventHandler(EventLoop *e1, int fd, xxxx){&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;char rxBuf[32];&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;int size;&lt;br&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;size = recv(fd, rxBuf, xxx);&lt;br&gt;&lt;br&gt;&lt;span style=&quot;white-space: pre;&quot;&gt; &lt;/span&gt;listener-&amp;gt;OnReceviceInterCore(rxBuf, size);&lt;br&gt;}" style="text;html=1;strokeColor=none;fillColor=none;align=left;verticalAlign=top;whiteSpace=wrap;rounded=0;fontSize=11;fontFamily=Helvetica;fontColor=default;" parent="qgbftZ3iaYtYM-1pswJt-33" vertex="1">
