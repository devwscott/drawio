        <mxCell id="9X4YrfVrGWDHbrTyHEu1-35" value="#ifndef APPLICATION_H&#xa;#define APPLICATION_H&#xa;&#xa;&#xa;#include &lt;string&gt;&#xa;#include &lt;memory&gt;&#xa;#include &quot;MQTTMessageParserFactory.h&quot;&#xa;&#xa;&#xa;class Application {&#xa;private:&#xa;    std::shared_ptr&lt;MQTTMessageParserFactory&gt; parserFactory_;&#xa;&#xa;&#xa;public:&#xa;    Application(const std::shared_ptr&lt;MQTTMessageParserFactory&gt;&amp; parserFactory);&#xa;&#xa;&#xa;    void handleMessage(const std::string&amp; topic, const std::string&amp; message);&#xa;};&#xa;&#xa;&#xa;#endif  // APPLICATION_H" style="text;whiteSpace=wrap;html=1;fillColor=#f5f5f5;fontColor=#333333;strokeColor=#666666;" parent="9X4YrfVrGWDHbrTyHEu1-30" vertex="1">
